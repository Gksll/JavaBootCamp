			System.out.println("Id: "+user.Id);
			System.out.println("First Name:"+user.firstName);
			System.out.println("Last Name:"+user.lastName);
			System.out.println("City:"+user.City);
		}
		
		
		
		
		
		
		
		
		
		
		
		
