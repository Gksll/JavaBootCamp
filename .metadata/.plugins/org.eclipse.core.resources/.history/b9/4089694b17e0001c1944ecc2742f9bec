package basics;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
	
		// 6 = 1,2,3
		Scanner girdi = new Scanner(System.in);
		System.out.print("bir sayi giriniz:");
		int sonuc = girdi.nextInt();
	
		int [] bolenler= new int[sonuc];
		
		for(int i =1; i<sonuc;i++) {
			if(sonuc%i==0) {
				int p=0;
				bolenler[p]=i;
			}
		}
	}

}
