
public class Main {

	public static void main(String[] args) {
		Category c1 = new Category();
		CategoryManager manager = new CategoryManager();
		c1.setId(1);
		c1.setDescription("1 numaralÄ± kategori");
		c1.setName("Back-End");
		manager.add(c1);
		manager.add(c1);
		manager.delete(c1);
		
		for (Category c : manager.GetAll()) {
			System.out.println(c.getName());
		}

	}

}
